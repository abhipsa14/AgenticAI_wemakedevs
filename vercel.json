{
  "version": 2,
  "builds": [
    {
      "src": "api/index.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.py"
    }
  ],
  "installCommand": "PIP_NO_CACHE_DIR=1 pip install -r requirements-vercel.txt",
  "env": {
    "PIP_NO_CACHE_DIR": "1"
  }
}
